{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {},
  "files": {
    "loopback#static": [
      {
        "params": "$!../client"
      },
      {
        "params": "$!../.tmp"
      },
      {
        "params": "$!../bower_components",
        "paths": "/bower_components"
      }
    ]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "loopback#errorHandler": {}
  }
}
